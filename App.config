<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.7.2" />
    </startup>
  <appSettings>
    <!-- Поставщик -->
    <add key="provider" value="System.Data.SqlClient"/>
    <!-- Строка подключения -->
    <add key="conStr" value="Data Source=(local)\SQLEXPRESS;
         Initial Catalog=Railway;
         Integrated Security=True;
         Pooling=False"/>
  </appSettings>
</configuration>